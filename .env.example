# Contentful Configuration
# Get these from https://app.contentful.com

# Your Contentful Space ID (from Settings → API keys)
VITE_CONTENTFUL_SPACE_ID=your_space_id_here

# Content Delivery API access token (read-only, from Settings → API keys → Content delivery / preview tokens)
VITE_CONTENTFUL_ACCESS_TOKEN=your_delivery_token_here

# Content Management API token (for updating claims, from Settings → API keys → Content management tokens)
VITE_CONTENTFUL_MANAGEMENT_TOKEN=your_management_token_here

# Environment (usually 'master')
VITE_CONTENTFUL_ENVIRONMENT=master
